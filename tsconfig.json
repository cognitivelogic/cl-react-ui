{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": false,
      "removeComments": true,
      "sourceMap": true,
      "strictNullChecks": false,
      "lib": [ "es6", "dom" ],
      "target": "es5"
    },
    "include": [
        "node_modules/@types/**/*.d.ts",
        "src/**/*.ts"
    ]
}
